{
  "name": "pdf-dicer",
  "version": "0.2.0",
  "description": "Split PDF files into many based on barcode separators",
  "main": "index.js",
  "engines": {
    "node": "4.3.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hash-bang/pdf-dicer.git"
  },
  "license": "MIT",
  "author": "Matt Carter <m@ttcarter.com>",
  "bugs": {
    "url": "https://github.com/hash-bang/pdf-dicer/issues"
  },
  "homepage": "https://github.com/hash-bang/pdf-dicer#readme",
  "scripts": {
    "test": "FORCE_COLOR=t tape ./test/**/*.test.js | tap-diff",
    "test:debug": "DEBUG=test FORCE_COLOR=t tape ./test/**/*.test.js | tap-diff",
    "playground": "npm run playground:generate && npm run playground:join",
    "playground:generate": "DEBUG=playground,generate-pdf GENERATE=true node ./playground/playground.js",
    "playground:join": "DEBUG=playground,pdf-join GENERATE=false node ./playground/playground.js",
    "quagga:demo": "./node_modules/http-server/bin/http-server ./playground/quagga-demo & node ./playground/quagga-demo/open.js"
  },
  "dependencies": {
    "async-chainable": "^2.2.3",
    "fs-extra": "^4.0.2",
    "lodash": "^4.17.4",
    "pdf-image": "^1.1.0",
    "quagga": "^0.12.1",
    "scissors": "^0.2.3",
    "temp": "^0.8.3"
  },
  "devDependencies": {
    "bluebird": "^3.5.1",
    "bwip-js": "^1.6.0",
    "debug": "^3.1.0",
    "http-server": "^0.10.0",
    "openurl": "^1.1.1",
    "pdf-merge": "^1.0.2",
    "pdfkit": "^0.8.3",
    "tap-diff": "^0.1.1",
    "tape": "^4.8.0",
    "uid": "0.0.2"
  }
}
